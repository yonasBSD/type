{
	"headers": [
		{
			"source": "/site.webmanifest",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/manifest+json"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=86400, s-maxage=86400"
				}
			]
		},
		{
			"source": "/(.*[\\.svg|png|woff|woff2])",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=1200000, s-maxage=1200000"
				}
			]
		}
	]
}